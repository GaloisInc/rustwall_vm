    <linkfile dest="tools/elfloader" src="elfloader-tool"/>
    <linkfile dest="tools/kbuild" src="kbuild-tool"/>
  </project>

  <!-- replce libc in Ruswall's Cargo.toml -->
  <project remote="galois" name="rustwall.git" path="projects/vm/components/Firewall/rustwall" revision="no_std" />

  <!-- link to specific version of libc inside smoltcp -->
  <project remote="galois" name="rs_smoltcp.git" path="projects/smoltcp" revision="master"/>

  
